{
  "info": {
    "name": "OAuth Authentication API",
    "description": "Collection for testing OAuth authentication endpoints",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8000",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Google OAuth Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"token\": \"google-oauth-access-token-here\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/auth/google",
          "host": ["{{baseUrl}}"],
          "path": ["api", "auth", "google"]
        }
      },
      "response": []
    },
    {
      "name": "Facebook OAuth Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"token\": \"facebook-oauth-access-token-here\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/auth/facebook",
          "host": ["{{baseUrl}}"],
          "path": ["api", "auth", "facebook"]
        }
      },
      "response": []
    },
    {
      "name": "Refresh Token",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"refresh_token\": \"jwt-refresh-token-here\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/auth/refresh",
          "host": ["{{baseUrl}}"],
          "path": ["api", "auth", "refresh"]
        }
      },
      "response": []
    },
    {
      "name": "Logout",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer jwt-access-token-here"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/auth/logout",
          "host": ["{{baseUrl}}"],
          "path": ["api", "auth", "logout"]
        }
      },
      "response": []
    },
    {
      "name": "Get User Profile",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer jwt-access-token-here"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/users/me",
          "host": ["{{baseUrl}}"],
          "path": ["api", "users", "me"]
        }
      },
      "response": []
    },
    {
      "name": "Update User Profile",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer jwt-access-token-here"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"avatar\": \"https://example.com/new-avatar.jpg\",\n  \"roles\": [\"ROLE_USER\", \"ROLE_MODERATOR\"]\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/users/me",
          "host": ["{{baseUrl}}"],
          "path": ["api", "users", "me"]
        }
      },
      "response": []
    },
    {
      "name": "Get User by ID",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/users/1",
          "host": ["{{baseUrl}}"],
          "path": ["api", "users", "1"]
        }
      },
      "response": []
    }
  ]
}
